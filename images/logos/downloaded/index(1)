$(document).ready(function() {
    $(".txtElqFldstateOrProvince").change(function() {
        if ($("select option:selected").hasClass('canadian')) {
            $('.add-email').prop('checked', false);
        }
        else {
          $('.add-email').prop('checked', true);
      }
  }); 
});
