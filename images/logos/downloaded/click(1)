	<script> 
        jQuery(document).ready(function() { 
        	$('a',$('.copyright')).click(function(){
        		if(window.WebMetrics){
        			var pageName = WebMetrics.getMetaTagValue('DCSext.wtPN');
        			dcsMultiTrack('DCSext.wtLinkLoc' , pageName+'_Footer',
    						'DCSext.wtLinkName' ,pageName + $(this).attr('class'),
    						'DCSext.svl' ,'2');
				}
        	});
                
        $('input.emailoptional').each(function(){
			if(this.value == ''){
				this.value='Enter your email address'; 
			}
		});
        $('input.emailoptional').click(function(){
			var inputvalue= this.value;
			if(inputvalue == 'Enter your email address'){
				this.value='';
			}
		});
        });
	</script> 
	
