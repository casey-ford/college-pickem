	<script>
	
	function checkOverlay()
	 {
		 if( $('#addressButton1').is(':visible')) {
			 if(window.WebMetrics){
					WebMetrics.Overlay.DCSext.wtSuccessFlag	= "-1";
					WebMetrics.overlayEventDispatch("SMBID_AddressValidation_Address_Enter");
				}		
			}
			if( $('#phoneBTN').is(':visible')) {
				if(window.WebMetrics){
					WebMetrics.Overlay.DCSext.wtSuccessFlag	= "-1";
					WebMetrics.overlayEventDispatch("SMBID_AddressValidation_TelephoneNumber_Enter");
				}
			}
			if( $('#singleCorrectedAddress').is(':visible')) {
				if(window.WebMetrics){
					WebMetrics.Overlay.DCSext.wtSuccessFlag	= "-1";
					WebMetrics.overlayEventDispatch("SMBID_AddressValidation_Single_Accept");
				}
			}
	 }

	function createAddressCookie(data){
		/*Code for July release - Save location data in Cookies start*/
		var postalCodeZip;
		var city;
		var stateCode;
		var stateFullName;
		var region;
		var cookieName = "address_overlay_cookie";

		postalCodeZip = data['postalCode'];
		city = data['city'];
		stateCode = data['state'];
		stateFullname = data['stateFullName'];
		region = data['geoLocation'];
		if(region == "13states" || region == "9states" || region  == "" || region  == null){
			region = "22states";
		}
		else{
			region = "28states";
		}
		var cookieString = "";
		
		if(city != null && city != ""){
			cookieString += "cookie_city%"+city+"|";
		}
		
		if(stateCode != null && stateCode != ""){
			cookieString += "cookie_state%"+stateCode+"|";
		}
		
		if(postalCodeZip != null && postalCodeZip != ""){
			cookieString += "cookie_postalcode%"+postalCodeZip+"|";
		}
		
		if(cookieString == ""){
			cookieString = "cookie_city%Dallas|cookie_state%TX|cookie_postalcode%75202|";
		}
		
		if(region != null && region != ""){
			cookieString += "cookie_geolocation%"+region;
		}
		
		address_overlay_cookie.checkAddress(cookieName, cookieString);
		
		
        /*Code for July release - Save location data in Cookies end*/		

	}
	
	
		jQuery(document).ready(function() { 
			
			
			$('input',$('#serviceAddress')).bind('keypress',function (event){
				if (event.keyCode === 13){
					if( $('#optionalAddressBtn').is(':visible')) {
					$('#optionalAddressBtn').trigger('click');
					}
				}
			});
			
			
			$('#btnEnableGo').click(function(e){
				
				if(window.WebMetrics){
					WebMetrics.Overlay.DCSext.wtSuccessFlag	= "1";
					WebMetrics.overlayEventDispatch("SMBID_AddressValidation_TelephoneNumber_Enter");
				}
			});	
			
			$('#enableGo').click(function(e){
				
				if(window.WebMetrics){
					WebMetrics.Overlay.DCSext.wtSuccessFlag	= "1";
					WebMetrics.overlayEventDispatch("SMBID_AddressValidation_Address_Enter");
				}
			});	
			
			
			
			$('input',$('#showphoneNumber')).bind('keypress',function (event){
				if (event.keyCode === 13){
					if( $('#btnChangeAddress').is(':visible')) {
					$('#btnChangeAddress').trigger('click');
					}
				}
			});
			
			$('input.optional').each(function(){
				if(this.value == ''){
					this.value='Optional'; 
				}
			});
			
			$('input.optional').click(function(){
				var inputvalue= this.value;
				if(inputvalue == 'Optional'){
					this.value='';
				}
			});
			
			$('#loading1').hide();
			
			$('#optionalWireless').click(function(e){
  				e.preventDefault();
 				var pCode = $("#postalCode").val();
				if('60290' == pCode){
					$('#validateAddressBtn').trigger('click');	
				}else{
					$('#addressValidationContent').hide();
					$('#serviceAddress').hide();
					$('#locationprovider').hide();
					$('#wirelesspresent').addClass('wirelessPresentInSession');
					$('#wirelesspresent').removeClass('continueWarning');
					$('#warningSaveCart').hide();
 					$('#warningSaveCartwithwireless').show();
					$('.wirelessPresentInSession').unbind('click');
					$('.wirelessPresentInSession').click(function(e){
						$('#validateAddressBtn').trigger('click');
						$('#saveCartOverlay').hide();
						$('#warningSaveCart').hide();
						$('#warningSaveCartwithwireless').hide();
						$('#saveCartOverlayZip').hide();
						$('#warningSaveCartZip').hide();
						$('#showphoneNumber').hide();
					});
				}
				
			});
			
			
			$('#optionalAddressBtn').click(function(e){
				e.preventDefault();
				$('#validateAddressBtn').trigger('click');
			});
		
			$('#btnChangeAddress').click(function(e){
				var pageName = WebMetrics.getMetaTagValue('DCSext.wtPN');
				WebMetrics.Overlay.DCSext.wtB2BTelephoneNum = $("#telephoneNumber_text").val();
				WebMetrics.Overlay.DCS.dcsuri = "/smallbusiness/overlay/addressEligibility.jsp";
				WebMetrics.overlayEventDispatch(pageName+"_OverlayAddressModalBusinessTelephoneNumberEntry_Go");
				
				e.preventDefault();
				$('#validateAddressBtn').trigger('click');
			});

			$('.wirelessPresentInSession').click(function(e){
				$('#validateAddressBtn').trigger('click');
				$('#saveCartOverlay').hide();
				$('#warningSaveCart').hide();
				$('#saveCartOverlayZip').hide();
				$('#warningSaveCartZip').hide();
				$('#showphoneNumber').hide();
 			});
			
			// gear implementation 
			 function checkNumber() {
				 if($('#telephoneNumber_text').val().indexOf('_') >-1 || $('#telephoneNumber_text').val() == "" || $('#telephoneNumber_text').val().length < 12) {
					 $('.goButton_enable').hide();
						$('.goButton_disable').show();
					}
					else{
						$('#telephoneNumber_text').val().replace( /(\d{3})(\d{3})(\d{4})/, '$1-$2-$3' );
						 telephoneNumber = $('#telephoneNumber_text').val();
						 var array_tel_num=telephoneNumber.split("");
						 if(array_tel_num[11] == array_tel_num[10])
							{	
								//alert("phone number field cannot have same value for all the 10 digits");
								}
							
						 else{
						$('.goButton_enable').show();
						$('.goButton_disable').hide();
				}
			}
				}
			
			
			 $('#telephoneNumber_text').bind('keydown keyup change focus',function(event){
					if($('#telephoneNumber_text').val().indexOf('_') >-1 || $('#telephoneNumber_text').val() == "" || $('#telephoneNumber_text').val().length < 12) {
						
						 $('.goButton_enable').hide();
							$('.goButton_disable').show();
						}
						else{
							
							$('#telephoneNumber_text').val().replace( /(\d{3})(\d{3})(\d{4})/, '$1-$2-$3' );
							 telephoneNumber = $('#telephoneNumber_text').val();
							 var array_tel_num=telephoneNumber.split("");
							 var flag = true;
							 for (count=1;count<12;count++){
								 if(array_tel_num[count]!='-'){
								 if(array_tel_num[0] != array_tel_num[count])
									 {
									 	flag=false;
									 }
								 }
								}
							 if(flag && event.type!= 'focus' && ((event.keyCode>=48 && event.keyCode<=68)||(event.keyCode>=96 && event.keyCode<=105)))
									{
								 	//alert("event code#####"+event.keyCode);
									alert("Business Telephone number field cannot have all the 10 digits same.");
									$('.goButton_enable').hide();
									$('.goButton_disable').show();
									}
								
							 else{
								$('.goButton_enable').show();
								$('.goButton_disable').hide();
							 }
				}	
				});
			$('#telephoneNumber_text').mask("?999-999-9999");
			
			
			$('#addressReturnBtn').click(function(e){
				e.preventDefault();
				var selectedAddressOption = $('input:checked',$('#addressOptionsContainer'));
				$('#address1').val($(selectedAddressOption).data('streetAddress').replace("&#039;","'"));
				$('#address2').val($(selectedAddressOption).data('addressLine2'));
				$('#rangedAddressOption').val($(selectedAddressOption).data('streetAddress'));
				$('#city').val($(selectedAddressOption).data('city'));
				$('#state').val($(selectedAddressOption).data('state'));
				$('#postalCode').val($(selectedAddressOption).data('postalCode'));
				var selectedRadioid = $('input:checked',$('#addressOptionsContainer'))[0].id;
				selectedRadioid = selectedRadioid.replace('optionalAddressRadio','');
				if($('#streetOption'+selectedRadioid).length >0){
					var enteredStreetNumber=$('#streetOption'+selectedRadioid).val();
					var addrress1 =$('#rangedAddressOption').val()
					addrress1 =new String(addrress1);
					var addSub = addrress1.split(' ');
					addSub[0] = enteredStreetNumber;
					$('#address1').val(addSub.join(' '));
				}
				$('#validateAddressBtn').trigger('click');
			});
			
			$("#multiAddressErrorBtn").click(function(e){
				e.preventDefault();
				$('#addressErrorBtn').click();
			});
			
			$("#multiAttemptAddressErrorBtn").click(function(e){
				e.preventDefault();
				$('#addressErrorBtn').click();
			});
			
			$('#addressErrorBtn').click(function(e){
				e.preventDefault();
				$('input.optional').each(function(){
					if(this.value == ''){
						this.value='Optional'; 
					}
				});
				$('#errorContainer').hide();
				$('#addressOptions').hide();
				$('#attemptFailure').hide();
				$('#serviceAddress').show();
				
				 var pageName = WebMetrics.getMetaTagValue('DCSext.wtPN');
				if($('#multiAddressErrorBtn').hasClass('singleReturnAddress')){
					WebMetrics.Overlay.DCSext.wtLinkLoc = pageName + '_Overlay_AddressModalSingleCorrectedAddressReturned_Body';
					WebMetrics.Overlay.DCS.dcsuri = "/smallbusiness/overlay/addressEligibility.jsp";
					WebMetrics.Overlay.DCSext.wtSuccessFlag="0";
					WebMetrics.overlayEventDispatch('SMBID_AddressValidation_Single_Accept');
				}else if($('#multiAddressErrorBtn').hasClass('rangedAddressReturn')){
					WebMetrics.Overlay.DCSext.wtLinkLoc = pageName + '_Overlay_AddressModalMultipleAddressRangesReturned_Body';
					WebMetrics.Overlay.DCS.dcsuri = "/smallbusiness/overlay/addressEligibility.jsp";
					WebMetrics.Overlay.DCSext.wtSuccessFlag="0";
					WebMetrics.overlayEventDispatch('SMBID_AddressValidation_MultipleRanges_Accept');
				}else if($('#multiAddressErrorBtn').hasClass('multipleAddressReturn')){
					WebMetrics.Overlay.DCSext.wtLinkLoc = pageName + '_Overlay_AddressModalMultipleAddressesReturned_Body';
					WebMetrics.Overlay.DCS.dcsuri = "/smallbusiness/overlay/addressEligibility.jsp";
					WebMetrics.Overlay.DCSext.wtSuccessFlag="0";
					WebMetrics.overlayEventDispatch('SMBID_AddressValidation_Multiple_Accept');
				}else if($("#multiAttemptAddressErrorBtn").hasClass('13states')){
					WebMetrics.Overlay.DCSext.wtLinkLoc = pageName + '_Overlay_AddressModalContactOptions13States_Body';
					WebMetrics.Overlay.DCS.dcsuri = "/smallbusiness/overlay/addressEligibility.jsp";
					WebMetrics.Overlay.DCSext.wtSuccessFlag="0";
					WebMetrics.overlayEventDispatch('SMBID_AddressValidation_FailedAddressCheck_Accept');
				}else if($("#multiAttemptAddressErrorBtn").hasClass('28states')){
					WebMetrics.Overlay.DCSext.wtLinkLoc = pageName + '_Overlay_AddressModalContactOptions28States_Body';
					WebMetrics.Overlay.DCS.dcsuri = "/smallbusiness/overlay/addressEligibility.jsp";
					WebMetrics.Overlay.DCSext.wtSuccessFlag="0";
					WebMetrics.overlayEventDispatch('SMBID_AddressValidation_FailedAddressCheck_Accept');
				}else if($("#multiAttemptAddressErrorBtn").hasClass('9states')){
					WebMetrics.Overlay.DCSext.wtLinkLoc = pageName + '_Overlay_AddressModalContactOptions9States_Body';
					WebMetrics.Overlay.DCS.dcsuri = "/smallbusiness/overlay/addressEligibility.jsp";
					WebMetrics.Overlay.DCSext.wtSuccessFlag="0";
					WebMetrics.overlayEventDispatch('SMBID_AddressValidation_FailedAddressCheck_Accept');
				}else{
					WebMetrics.Overlay.DCS.dcsuri = "/smallbusiness/overlay/addressEligibility.jsp";
					WebMetrics.Overlay.DCSext.wtSuccessFlag="0";
					WebMetrics.overlayEventDispatch('SMBID_AddressValidation_NotFound_Accept');
				}
				
			});
			
			jQuery.ajaxSetup({accepts:'application/json'}) ;
			jQuery('#validateAddressBtn',$('#changeAdreesEligibility')).click(function(event) {
				$('input.optional').each(function(){
					if(this.value == 'Optional'){
						this.value=''; 
					}
				});
				var data = {};
				jQuery('input',$('#changeAdreesEligibility')).each(function(){
					data[$(this).attr('name')] = $(this).val();
				});
				
				jQuery('input[type="checkbox"]',$('#changeAdreesEligibility')).each(function(index,thisCheckbox){
					if(thisCheckbox.checked){
						data[$(thisCheckbox).attr('name')]="true";
					}else{
						data[$(thisCheckbox).attr('name')]="false";
					}
				});
				
				jQuery('select',$('#changeAdreesEligibility')).each(function(){
					data[$(this).attr('name')] = $(this).val();
				});
				//Pass the name/value pair of submit button as well.
				data[jQuery('#validateAddressBtn').attr('name')] = jQuery('#validateAddressBtn').attr('value');
				var url = '/smallbusiness/common/serviceAvailabilitySuccess.jsp';
					event.preventDefault();
					jQuery.ajax({
						url : url,
						type : 'POST',
						data : data,
						success : validationSuccess,
						error : validationError,
						dataType : 'json',
						beforeSend : function(xhr) {
							$('#addressValidationContent').hide();
							$('#loading1').show();
							/* In formhandler check value of this header to figure out if its 
							a normal submiission or its a AJAX request and take nacessary action.*/
							xhr.setRequestHeader('Accept','application/json');
						}
					});
					return false;
				});
			
			/* Adding the reporing tags for address eligibility */
			var knownAddress = false;
			if($('#postalCode').val()!== ""){
				knownAddress=true;
			}
			$('#optionalAddressBtn').click(function(e){
				var pageName = WebMetrics.getMetaTagValue('DCSext.wtPN');
				if(window.WebMetrics)
				{
					WebMetrics.Overlay.DCSext.wtB2BAddressModalCheckState= $('#state').val();
					WebMetrics.Overlay.DCSext.wtB2BAddressModalCheckCity=$('#city').val(); 
					WebMetrics.Overlay.DCSext.wtB2BAddressModalCheckZipCode= $('#postalCode').val();
						
					//pass this var only when fedTaxID is visible
					if($('#federalTaxId:visible').length > 0){
						if($('#federalTaxId').is(':checked')){
							WebMetrics.Overlay.DCSext.wtB2BHaveFederalTaxIDInd = "true";
						}else{
							WebMetrics.Overlay.DCSext.wtB2BHaveFederalTaxIDInd = "false";
						}
					}
					
					if($('#existCustomer').is(':checked')){
						WebMetrics.Overlay.DCSext.wtB2BExistingCustomerInd = "true";
						dcsMultiTrack('DCSext.wtB2BBusinessValidationMethod',"Existing customer"
					      );
					}else{
						WebMetrics.Overlay.DCSext.wtB2BExistingCustomerInd = "false";
					}
										
					WebMetrics.Overlay.DCSext.wtB2BAddressConfirmationMethod = "Full Address";
					WebMetrics.Overlay.DCSext.wtB2BBIDProductFilterMethod    = "Confirmed by Address";
					//WebMetrics.Overlay.DCSext.wtB2BEstimatedZipCode		  = " ";
					WebMetrics.Overlay.DCSext.wtB2BConfirmedZipCode		  = '60290';
					WebMetrics.Overlay.DCSext.wtSuccessFlag				  = "1";
					
					WebMetrics.Overlay.DCS.dcsuri = "/smallbusiness/overlay/addressEligibility.jsp";
					WebMetrics.overlayEventDispatch("SMBID_AddressValidation_Address_Enter");
					
				}
			});
			
			$('#addressReturnBtn').click(function(){
				var pageName = WebMetrics.getMetaTagValue('DCSext.wtPN');
				if($(this).hasClass('singleReturnAddress')){
					if(window.WebMetrics)
					{ 	
						WebMetrics.Overlay.DCSext.wtLinkLoc = pageName + '_Overlay_AddressModalSingleCorrectedAddressReturned_Body';
						WebMetrics.Overlay.DCS.dcsuri = "/smallbusiness/overlay/addressEligibility.jsp";
						WebMetrics.Overlay.DCSext.wtSuccessFlag="1";
						WebMetrics.overlayEventDispatch('SMBID_AddressValidation_Single_Accept');
						
						
					}
				}
				if($(this).hasClass('rangedAddressReturn')){
					if(window.WebMetrics)
					{ 	
						WebMetrics.Overlay.DCSext.wtLinkLoc = pageName + '_Overlay_AddressModalMultipleAddressRangesReturned_Body';
						WebMetrics.Overlay.DCS.dcsuri = "/smallbusiness/overlay/addressEligibility.jsp";
						WebMetrics.Overlay.DCSext.wtSuccessFlag="1";
						WebMetrics.overlayEventDispatch('SMBID_AddressValidation_MultipleRanges_Accept');
						
					}
				}
				if($(this).hasClass('multipleAddressReturn')){
					if(window.WebMetrics)
					{ 	
						WebMetrics.Overlay.DCSext.wtLinkLoc = pageName + '_Overlay_AddressModalMultipleAddressesReturned_Body';
						WebMetrics.Overlay.DCS.dcsuri = "/smallbusiness/overlay/addressEligibility.jsp";
						WebMetrics.Overlay.DCSext.wtSuccessFlag="1";
						WebMetrics.overlayEventDispatch('SMBID_AddressValidation_Multiple_Accept');
						
					}
				} 
				
			});
			
			
			
		});
		
		function validationSuccess(data){
			
			$('#evaluateForBTNNumber').val("");
			if(data['errors'] != undefined || data['systemError'] != undefined)
			{
				/* console.log(data['failureCount']); */
				var failureCount = data['failureCount'];
				/* console.log(data['geoLocation']); */
				$('#addressReturnBtn').removeClass();
				$('#multiAddressErrorBtn').removeClass()
				var geoLocation = data['geoLocation'];
				if(failureCount > 3){
					$('#loading1').hide();
					$('#serviceAddress').hide();
					$('#addressOptions').hide();
					$('#errorContainer').hide();
					$('#showphoneNumber').hide();
					$('#addressValidationContent').show();
					$('#multiAttemptAddressErrorBtn').removeClass();
					$('#multiAttemptAddressErrorBtn').addClass(geoLocation);
					$('#attemptFailure').show().find("div[id$=states]").hide().parent().find('div#'+geoLocation).show();
					if(geoLocation == "13states"){
						WebMetrics.Overlay.DCS.dcsuri = "/smallbusiness/overlay/addressEligibility.jsp";
		        		WebMetrics.overlayPageDispatch(WebMetrics.getPageName() + "  Overlay Address Modal Contact Options 13 States");
					}else if(geoLocation == "28states"){
						WebMetrics.Overlay.DCS.dcsuri = "/smallbusiness/overlay/addressEligibility.jsp";
		        		WebMetrics.overlayPageDispatch(WebMetrics.getPageName() + "  Overlay Address Modal Contact Options 28 States");
					}else if(geoLocation == "9states"){
						WebMetrics.Overlay.DCS.dcsuri = "/smallbusiness/overlay/addressEligibility.jsp";
		        		WebMetrics.overlayPageDispatch(WebMetrics.getPageName() + "  Overlay Address Modal Contact Options 9 States");
					}
					return;
				}else{
					$('#addressErrorContainer').html("");
					var city = $('#city').val();
					city=city.replace("&#039;","'");
					for( i=0;i<data['errors'].length;i++ ){
						var errorMsg= data['errors'][i].split(':');
						if(errorMsg[1])
							$('#addressErrorContainer').append('<div>'+errorMsg[1]+'</div>');
					}
					$('#addressValidationContent').show();
					$('#loading1').hide();
					
					$('#errorAddress1').text($.trim($('#address1').val()));
					$('#errorAddress2').text($.trim($('#address2').val()));
					$('#errorCitySateZip').text($.trim(city) + ", " + $('#state').val() + " " + $('#postalCode').val());
					$('#attemptFailure').hide();
					$('#serviceAddress').hide();
					$('#addressOptions').hide();
					$('#errorContainer').show();
					$('#showphoneNumber').hide();
					
					WebMetrics.Overlay.DCS.dcsuri = "/smallbusiness/overlay/addressEligibility.jsp";
	        		WebMetrics.overlayPageDispatch(WebMetrics.getPageName() + " Overlay Address Modal Address Not Found Pg");
							
				}
			}else if(data['showPhoneNumber']=== 'true'){
				$('#loading1').hide();
				$('#attemptFailure').hide();
				$('#serviceAddress').hide();
				$('#addressOptions').hide();
				$('#errorContainer').hide();
				$('#addressValidationContent').show();
				$('#showphoneNumber').show();
				
				WebMetrics.Overlay.DCS.dcsuri = "/smallbusiness/overlay/addressEligibility.jsp";
        		WebMetrics.overlayPageDispatch(WebMetrics.getPageName() + " Overlay Address Modal Business Telephone Number Entry");
        		createAddressCookie(data);
				$('#telephoneNumber_text').focus();
			}else if(!$.isArray(data)){
				var isFamilyPage = $('#FAMILY_PAGE').val();
				var isChildURL = false;
				var targetURL = $('#SeviceEligibiltyLightBox').data('srcURL');
				if(targetURL == undefined)
					targetURL=window.location.href.replace("changeLocation=true","");
				
				var geoLocation = data['geoLocation'];
				
				if(geoLocation == "13states"){
					WebMetrics.Overlay.DCSext.wtB2BStateGroupName = "13 State";
				}else if(geoLocation == "28states"){
					WebMetrics.Overlay.DCSext.wtB2BStateGroupName = "28 State";
				}else if(geoLocation == "9states"){
					WebMetrics.Overlay.DCSext.wtB2BStateGroupName = "9 State";
				}
				WebMetrics.Overlay.DCS.dcsuri = "/smallbusiness/overlay/addressEligibility.jsp";
				//WebMetrics.overlayEventDispatch(WebMetrics.getPageName() +"_Overlay_AddressModalValidation_Complete");
				
				$('.locationOverlay,.zipCodeOverlayChangeLoc').data('myplugin').closeOverlay();
				createAddressCookie(data);
				$('.close',$('#SeviceEligibiltyLightBox')).trigger("click");
				$('#processProducts').click();
				var src = "/smallbusiness/common/processing.jsp?successURL="+targetURL;
				$('#processingPage').attr("src",src);
	
			}else if ($.isArray(data)){
				$('#addressOptionsContainer').html("");
				if(data.length >15){
					$('#addressOptionsContainerMain').css('overflow-y','scroll');
					$('#addressOptionsContainerMain').css('max-height','400px');
					}
				for(var count = 0 ; count < data.length ; count++){
					var streetAddressString = data[count]['addressLine1'];
					var city = data[count]['city'];
					city=city.replace("&#039;","'");
					var state = data[count]['state'];
					var postalCode = data[count]['postalCode'];
					var addressLine2= data[count]['addressLine2'];
					addressLine2 = $.trim(addressLine2);
					var addressString = "";
					if( addressLine2 == ''){
						addressString = streetAddressString + ", " + city + ", " + state + " " + postalCode;
					} else {
						addressString = streetAddressString + ", " + addressLine2 + ", " + city + ", " + state + " " + postalCode;
					}
					var radioElement = $('<input>').attr({type: 'radio', name: 'addressInput', value: '', id: 'optionalAddressRadio'+count});
						radioElement.data('city',city);
						radioElement.data('state',state);
						radioElement.data('postalCode',postalCode);
						radioElement.data('streetAddress',streetAddressString);
					 	radioElement.data('addressLine2',addressLine2);
						radioElement.data('isRangedAddress','false');
					var radioLabel = $('<span>').html(addressString).addClass('label');
					if(data[count]['isRangedAddress'] == true){
						var streetInput = $('<span style="margin-left:10px;">').append($('<input>').attr({type: 'text', style:'width:80px;',name: 'streetInput', value: '',id: 'streetOption'+count}));
						var streetLabel = $('<span>').html('Updated Street Address').addClass('label');
						var streetElement =$('<div style="display:none;padding:10px;">').attr({id: 'streetElement'+count}).append(streetLabel).append(streetInput);
						radioElement.data('streetElement',streetElement);
						radioElement.data('isRangedAddress',data[count]['isRangedAddress']);
					}
					radioElement.click(function(){   
						if($(this).data('isRangedAddress') == true){
							$('div[id^=streetElement]').each(function(){
								$(this).hide();
							});
							$(this).data('streetElement').show();
						}
						$('input[id^="optionalAddressRadio"]').attr('checked',false);
						$(this).attr('checked',true);
					});
					
					$('#addressValidationContent').show();
					$('#loading1').hide();

					$('#addressOptionsContainer').append($('<div>').append(radioElement).append(radioLabel));
					if(data[count]['isRangedAddress'] == true){
						$('#addressOptionsContainer').append(streetElement);
					}	
					if(count == 0){
						if(data[count]['isRangedAddress'] == true){
							$('#addressReturnBtn').addClass('rangedAddressReturn');
							$('#multiAddressErrorBtn').addClass('rangedAddressReturn');
						}else{
							$('#addressReturnBtn').addClass('multipleAddressReturn');
							$('#multiAddressErrorBtn').addClass('multipleAddressReturn');
						}
						radioElement.attr('checked',true);
						radioElement.trigger('click');	
					}
					
				}
				$('#serviceAddress').hide();
				$('#errorContainer').hide();
				$('#attemptFailure').hide();
				$('#showphoneNumber').hide();
				if((data.length == 1) && (data[0]['isRangedAddress'] != true)){	
					$('#addressOptionsContainerMain').hide();
					var streetAddressString = data[0]['addressLine1'];
					var city = data[0]['city'];
					city=city.replace("#039;","'");
					var state = data[0]['state'];
					var postalCode = data[0]['postalCode'];  
					var addressLine2= data[0]['addressLine2'];
					addressLine2 = $.trim(addressLine2);
					var addressString = "";
					addressString = city + ", " + state + " " + postalCode;
					$('#singleCorrectedAddressDetails').html("");
					$('#singleCorrectedAddressDetails').append($('<div>').append("<strong>"+streetAddressString+"</strong>"));
					if( addressLine2 != ''){
						$('#singleCorrectedAddressDetails').append($('<div>').append("<strong>"+addressLine2+"</strong>"));
					}
					$('#singleCorrectedAddressDetails').append($('<div>').append("<strong>"+addressString+"</strong>"));
					$('#addressReturnBtn').removeClass();
					$('#multiAddressErrorBtn').removeClass();
					$('#addressReturnBtn').addClass('singleReturnAddress');
					$('#multiAddressErrorBtn').addClass('singleReturnAddress')
					$('#singleCorrectedAddress').show();
					
					WebMetrics.Overlay.DCS.dcsuri = "/smallbusiness/overlay/addressEligibility.jsp";
	        		WebMetrics.overlayPageDispatch(WebMetrics.getPageName() + " Overlay Address Modal Single Corrected Address Returned");
					
				}else{
					$('#addressOptions').show();
					$('#singleCorrectedAddress').hide();
					$('#addressOptionsContainerMain').show();
					WebMetrics.Overlay.DCS.dcsuri = "/smallbusiness/overlay/addressEligibility.jsp";
	        		WebMetrics.overlayPageDispatch(WebMetrics.getPageName() + "  Overlay Address Modal Address Multiple Addresses Returned");
				}
				$('#addressOptions').show();
				
				

			}

		}
		function validationError(jqXHR, textStatus, errorThrown) {
			
		}
	</script>


