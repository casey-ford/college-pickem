				$('#errorContainerZipCode').hide();
				//$('#addressOptions').hide();
			//	$('#attemptFailure').hide();
				$('#serviceZip').show();
				}); */
			
			jQuery.ajaxSetup({accepts:'application/json'}) ;
			jQuery('#validateZipBtn',$('#changeZipEligibility')).click(function(event) {
					
				if(!($('#existCustomerZip').is(':checked'))){
					$('#existCustomerZipChkbx').val('false');
					WebMetrics.Overlay.DCSext.wtB2BExistingCustomerInd = "false";
				}
				else{
					$('#existCustomerZipChkbx').val('true');
					WebMetrics.Overlay.DCSext.wtB2BExistingCustomerInd = "true";
				}
				$('input.optional').each(function(){
					if(this.value == 'Optional'){
						this.value=''; 
					}
				});
				var data = {};
				jQuery('input',$('#changeZipEligibility')).each(function(){
					data[$(this).attr('name')] = $(this).val();
				});
				
				
				//Pass the name/value pair of submit button as well.
				data[jQuery('#validateZipBtn').attr('name')] = jQuery('#validateZipBtn').attr('value');
				var url = '/smallbusiness/common/zipCodeAvailabilitySucess.jsp';
					event.preventDefault();
					jQuery.ajax({
						url : url,
						type : 'POST',
						data : data,
						success : validationSuccess,
						error : validationError,
						dataType : 'json',
						beforeSend : function(xhr) {
							$('#zipValidationContent').hide();
							$('#loadingZipPocessing').show();
							/* In formhandler check value of this header to figure out if its 
							a normal submiission or its a AJAX request and take nacessary action.*/
							xhr.setRequestHeader('Accept','application/json');
						}
					});
					return false;
				});
			
			
		
		function validationSuccess(data){
			$('#evaluateForBTNNumberZipOVerlay').val("");
			if(data['errors'] != undefined || data['systemError'] != undefined)
			{
				var failureCount = data['failureCount'];
				//$('#addressReturnBtn').removeClass();
				//$('#multiAddressErrorBtn').removeClass()
				var geoLocation = data['geoLocation'];
				if(failureCount > 3){
					$('#loadingZipPocessing').hide();
					$('#serviceZip').hide();
					//$('#addressOptions').hide();
					$('#errorContainerZipCode').hide();
					$('#showphoneNumberZip').hide();
					$('#zipValidationContent').show();
					//$('#multiAttemptAddressErrorBtn').removeClass();
					//$('#multiAttemptAddressErrorBtn').addClass(geoLocation);
					//$('#attemptFailure').show().find("div[id$=states]").hide().parent().find('div#'+geoLocation).show();
					if(geoLocation == "13states"){
						WebMetrics.Overlay.DCS.dcsuri = "/smallbusiness/overlay/addressEligibility.jsp";
						WebMetrics.Overlay.DCSext.wtSuccessFlag="0";
		        		WebMetrics.overlayEventDispatch("SMBID_AddressValidation_FailedAddressCheck_Accept");
					}else if(geoLocation == "28states"){
						WebMetrics.Overlay.DCSext.wtSuccessFlag="0";
						WebMetrics.Overlay.DCS.dcsuri = "/smallbusiness/overlay/addressEligibility.jsp";
		        		WebMetrics.overlayEventDispatch("SMBID_AddressValidation_FailedAddressCheck_Accept");
					}else if(geoLocation == "9states"){
						WebMetrics.Overlay.DCSext.wtSuccessFlag="0";
						WebMetrics.Overlay.DCS.dcsuri = "/smallbusiness/overlay/addressEligibility.jsp";
		        		WebMetrics.overlayEventDispatch("SMBID_AddressValidation_FailedAddressCheck_Accept");
					}
					return;
				}else{
					//$('#addressErrorContainer').html("");
					
					/* for( i=0;i<data['errors'].length;i++ ){
						var errorMsg= data['errors'][i].split(':');
						if(errorMsg[1])
							$('#addressErrorContainer').append('<div>'+errorMsg[1]+'</div>');
					} */
					$('#zipValidationContent').show();
					$('#loadingZipPocessing').hide();
					$('#errorContainerZipCode').show();
					$('#showphoneNumberZip').hide();
					WebMetrics.Overlay.DCSext.wtSuccessFlag="0";
					WebMetrics.Overlay.DCS.dcsuri = "/smallbusiness/overlay/addressEligibility.jsp";
	        		WebMetrics.overlayEventDispatch("SMBID_AddressValidation_NotFound_Accept");
							
				}
			}else if(data['showphoneNumberZip']=== 'true'){
				$('#loadingZipPocessing').hide();
				//$('#attemptFailure').hide();
				$('#serviceZip').hide();
			//	$('#addressOptions').hide();
				$('#errorContainerZipCode').hide();
				$('#zipValidationContent').show();
				$('#showphoneNumberZip').show();
				
				WebMetrics.Overlay.DCS.dcsuri = "/smallbusiness/overlay/addressEligibility.jsp";
        		WebMetrics.overlayEventDispatch("SMBID_AddressValidation_TelephoneNumber_Enter");
				createCookie(data);
				$('#telephoneNumber_text_zip').focus();
			}else if(!$.isArray(data)){
				var isFamilyPage = $('#FAMILY_PAGE').val();
				var isChildURL = false;
				var targetURL = $('#zipcodeOverlayLightBox').data('srcURL');
				if(targetURL == undefined)
					targetURL=window.location.href.replace("changeLocation=true","");
				
				var geoLocation = data['geoLocation'];
				
				if(geoLocation == "13states"){
					WebMetrics.Overlay.DCSext.wtB2BStateGroupName = "13 State";
				}else if(geoLocation == "28states"){
					WebMetrics.Overlay.DCSext.wtB2BStateGroupName = "28 State";
				}else if(geoLocation == "9states"){
					WebMetrics.Overlay.DCSext.wtB2BStateGroupName = "9 State";
				}
				
				var estZip = " ";
				var confZip = " ";
				var postalCode = $('input.toBeValidated:last').val();
				var pageName = WebMetrics.getMetaTagValue('DCSext.wtPN');
				
				'filter '+'true';
				'cookie '+'true';
				/* 
				
					
					
				
					
					
						
						
							
						
						
							
								filterMethod	   = "Confirmed by Zip";
								
								
									
									
								
									
									
										
										
											
										
										
											 */
												/* filterMethod = "Estimated by Zip";
												estZip = postalCode; */
											/* 
										
									
								
								confZip = postalCode;
							
						
					
				
				 */			
				
				/* if(confZip.length < 5)
					estZip = postalCode;
				
				
				 */
				WebMetrics.Overlay.DCSext.wtB2BAddressConfirmationMethod = "Zip Only";
				WebMetrics.Overlay.DCSext.wtB2BBIDProductFilterMethod    = "Confirmed by ZIP";
				WebMetrics.Overlay.DCSext.wtB2BConfirmedZipCode		  = postalCode;
				WebMetrics.Overlay.DCSext.wtSuccessFlag				  = "1";
				WebMetrics.Overlay.DCSext.wtLinkName					= pageName + " _ChangeLocation";
				WebMetrics.Overlay.DCSext.wtLinkLoc						= pageName + "_ChangeLocation_Body";
				
				WebMetrics.Overlay.DCS.dcsuri = "/smallbusiness/overlay/addressEligibility.jsp";
				WebMetrics.overlayEventDispatch("SMBID_AddressValidation_Address_Enter");
							
				//alert("overlay type"+$('#currentOverlay').val());
				if($('#currentOverlay').val() == 'verifyLocationOverlay'){
					$('.zipCodeOverlayChangeLoc').data('myplugin').closeOverlay();
				}
				else if($('#currentOverlay').val() == 'wirelessZipOverlay'){
					$('.zipCodeOverlay').data('myplugin').closeOverlay();
				}
				createCookie(data);
				$('#processProducts').click();
				
				var src = "/smallbusiness/common/processing.jsp?wireless=true&successURL="+targetURL;
				$('#processingPage').attr("src",src);
	
			}else if ($.isArray(data)){
				
				$('#serviceZip').hide();
				$('#errorContainerZipCode').hide();
				//$('#attemptFailure').hide();
				$('#showphoneNumberZip').hide();
				
		}
		}
		 function validationError(jqXHR, textStatus, errorThrown) {
			
		} });
	</script>


