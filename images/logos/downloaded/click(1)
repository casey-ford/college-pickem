	<script>
		var address_overlay_cookie = {
			setAddress : function(cname, cvalue) {
				var loc = window.location;
				//document.cookie = cname + "=" + cvalue + ";path=/;domain=" + loc.hostname;
				document.cookie = cname + "=" + cvalue
						+ ";path=/;domain=.att.com";
			},

			getAddress : function(cname) {
				var name = cname + "=";
				var ca = document.cookie.split(';');
				for (var i = 0; i < ca.length; i++) {
					var c = ca[i];
					while (c.charAt(0) == ' ')
						c = c.substring(1, c.length);
					if (c.indexOf("address_overlay_cookie") == 0) {
						return c.substring(name.length, c.length);
					}
				}
			},

			getKey : function(cname, key) {
				var cookie = this.getAddress(cname);
				var name = key + "%";
				var keys = cookie.split("|");
				for (var i = 0; i < keys.length; i++) {
					while (keys[i].charAt(0) == ' ')
						keys[i] = keys[i].substring(1, keys[i].length);
					if (keys[i].indexOf(key) == 0)
						return keys[i].substring(name.length, keys[i].length);
				}
			},

			checkAddress : function(cname, cvalue) {
				var addressCookie = this.getAddress(cname);
				if (addressCookie != cvalue) {
					this.setAddress(cname, cvalue);
				}
			}
		}
		var cityFromAkamai = 'CHICAGO';
		var stateFromAkamai = 'IL';
		var postalCodeFromAkamai = '60290';
		var geoLocationForAkamaiZip = '22states';
		var cookieName = "address_overlay_cookie";
		var cookie = address_overlay_cookie.getAddress(cookieName);
		if (cookie == "" || cookie == null) {
			if (cityFromAkamai != null && cityFromAkamai != ""
					&& stateFromAkamai != null && stateFromAkamai != ""
					&& postalCodeFromAkamai != null
					&& postalCodeFromAkamai != "") {
				address_overlay_cookie.setAddress(cookieName, "cookie_city%"
						+ cityFromAkamai + "|cookie_state%" + stateFromAkamai
						+ "|cookie_postalcode%" + postalCodeFromAkamai
						+ "|cookie_geolocation%"+geoLocationForAkamaiZip);
			} else {
				address_overlay_cookie
						.setAddress(
								cookieName,
								"cookie_city%Dallas|cookie_state%TX|cookie_postalcode%75202|cookie_geolocation%22states");
			}
		}
	</script>
	
