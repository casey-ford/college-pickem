		<div id="saveCartOverlay" style="display:none;">
			

 

	
	
	
	
	<script> 
        jQuery(document).ready(function() { 
        	//Added for Adobe fix
        	$('input.eeemail15').each(function(){
    			if(this.value == ''){
    				this.value='Enter your email address'; 
    			}
    		});
            $('input.eeemail15').click(function(){
    			var inputvalue= this.value;
    			if(inputvalue == 'Enter your email address'){
    				this.value='';
    			}
    		});
			
        	$('.btnSaveCartImg',$('#saveCartOverlay')).click(function(e){
        		e.preventDefault();
        		$('#btnSaveCart',$('#saveCartSubmit')).trigger('click');
        	});
        	$('#btnSaveCartOkImg',$('#saveCartOverlay')).click(function(e){
        		e.preventDefault();
        		if(false){
    				$('#flowType').val("withwireless");
    			}
        		if($('#flowType').val() == 'changeLocation'){
        			$('#changeAdreesEligibility').show();
					$('#saveCartOverlay').hide();
					$('#warningSaveCart').hide();
					$('#showphoneNumber').hide();
					$('#warningSaveCartwithwireless').hide();
        		}else if($('#flowType').val() == 'cancelCheckout'){
        			$('#cancelCheckoutSubmit',$('#cancelCheckoutLightBox')).trigger("click");
        		}else if($('#flowType').val() == 'macFlow'){
        			var srcURL = $('#SeviceEligibiltyLightBox').data('srcURL');
        			$('#urlforMac').val(srcURL);
        			$('#btnMacredirect').trigger("click");
        		}else if($('#flowType').val() == 'changeLocationwithWireless'){
        			$('.close',$('#SeviceEligibiltyLightBox')).trigger("click");
        		}
        		else{
        			$('.close',$('#SeviceEligibiltyLightBox')).trigger("click");
        		}
        	});
        	
        	jQuery.ajaxSetup({accepts:'application/json'}) ;
        	
			jQuery('#btnSaveCart',$('#saveCartSubmit')).click(function(event) {
			 	var data = {};
		        jQuery('input',$('#saveCartSubmit')).each(function(){
		        	data[$(this).attr('name')] = $(this).val();
		        });
		        jQuery('select',$('#saveCartSubmit')).each(function(){
		        	data[$(this).attr('name')] = $(this).val();
		        });
				//Pass the name/value pair of submit button as well.
		        data[jQuery('#btnSaveCart').attr('name')] = jQuery('#btnSaveCart').attr('value');
		        var url = '/smallbusiness/cart/saveCartResult.jsp';
					event.preventDefault();
					jQuery.ajax({
						url : url,
						type : 'POST',
						data : data,
						success : validationSuccess,
						error : validationError,
						dataType : 'json',
						beforeSend : function(xhr) {
							xhr.setRequestHeader('Accept','application/json');
							$('#loading').show();
							$('#saveCartContainer').hide();
						}
					});
					
					var wtPN = WebMetrics.getMetaTagValue('DCSext.wtPN');
					var email = $('#emailAddCart').val();
					if(email != undefined && email != null){
						WebMetrics.DCSext.wtB2BSaveCartEmailAddress = $('#emailAddCart').val();
						WebMetrics.dispatchReport( wtPN + "_SaveCartAndContinueLater");
					}
					return false;
			  });
			function validationSuccess(data){
				$('#loading').hide();
				$('#saveCartContainer').show();
				var responseData = data['errors'];
				var isCartSaved = data['alreadyCartSaved'];
				if(responseData != undefined){
					if(isCartSaved == true)
					{
						$('#infoDisplay').html("");
					    var infoMsg = responseData[0];
					    var infoArray= infoMsg.split(":");
						if(infoArray.length > 1){
							var infoMsgShow = '<div  class="infoMsgDiv" >'+infoArray[1]+'</div>';
						}else{
							var infoMsgShow = '<div  class="infoMsgDiv" >'+infoArray[0]+'</div>';
						}
						$('#infoDisplay').html(infoMsgShow);
						$('#serverReponse').html("");
						$('#alreadySaved').show();
						$('#defaultSubmit').hide();
						$('#isUpdateFlag').val('true');
						isCartSaved = false;
					}
					else{
						$('#serverReponse').html("");
						var errorMsg =responseData[0]
						var errorArray= errorMsg.split(":");
						if(errorArray.length > 1){
							var errorMsgShow = '<span class="inlineErrorMsg" style="color:red;">'+errorArray[1]+'</span>';
						}else{
							var errorMsgShow = '<span class="inlineErrorMsg" style="color:red;">'+errorArray[0]+'</span>';
						}
						$('#serverReponse').html(errorMsgShow);
						$('#infoDisplay').html("");
						$('#alreadySaved').hide();
						$('#defaultSubmit').show();
						/* $('#btnSaveCartImg').hide(); */
						$('#isUpdateFlag').val('false');
					}
				}
				else{
					$('#loading').hide();
					$('#saveCartContainer').show();
					$('#infoDisplay').html("");
					$('#initSavedCartMsg').hide();
					$('#savedCartMsg').show();
					
            		_satellite.track('Save Cart');
				}
			}
			
			function validationError(jqXHR, textStatus, errorThrown) {
			}
        	
        });
    </script>


	
