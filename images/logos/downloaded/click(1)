	<script> 
        jQuery(document).ready(function() { 
        	$('#optInEmailImg',$('#stayConnectedID')).click(function(e){
        		e.preventDefault();
        		$('#optInEmailSubmit',$('#stayConnectedID')).trigger('click');
        	});
        	
        	$('#stayConnectedID :text').keypress(function(event){
        	    var keycode = (event.keyCode ? event.keyCode : event.which);
        		if(keycode == '13'){
        			event.preventDefault();
        			$('#optInEmailImg',$('#stayConnectedID')).trigger('click');
        		}
        	});
        	
			jQuery.ajaxSetup({accepts:'application/json'}) ;
        	
			jQuery('#optInEmailSubmit',$('#stayConnectedID')).click(function(event) {
				if(window.WebMetrics){
					var pageName = WebMetrics.getMetaTagValue('DCSext.wtPN');
					dcsMultiTrack('DCSext.wtLinkLoc' , pageName+'_SocialMedia',
							'DCSext.wtEvent' ,pageName +'_EnterYourEmail_Submit',
							'DCSext.svl' ,'2');
				}
				
			 	var data = {};
		        jQuery('input',$('#stayConnectedID')).each(function(){
		        	data[$(this).attr('name')] = $(this).val();
		        });
		        
				//Pass the name/value pair of submit button as well.
		        data[jQuery('#optInEmailSubmit').attr('name')] = jQuery('#optInEmailSubmit').attr('value');
		        var url = '/smallbusiness/common/optInEmailAjax.jsp';
					event.preventDefault();
					jQuery.ajax({
						url : url,
						type : 'POST',
						data : data,
						success : optinEmailSuccess,
						error : optinEmailError,
						dataType : 'json',
						beforeSend : function(xhr) {
							xhr.setRequestHeader('Accept','application/json');
						}
					});
					return false;
			  });
			
			function optinEmailSuccess(data){
				var $spanInContext, $arrowInContext;
				if(data['emailSaved'] == 'true'){
					$('#emailSuccessMsgHolder').show();
					$('#emailErrorMsgHolder').hide();
					$spanInContext = $('.optInEmailSuccess');
					$arrowInContext = $('.footerConfirmMsgArrow');
					$('#optInEmailOverlay').myplugin({
			            'backDropId' : 'optInEmailDropBox',
			            'lightBoxId' : 'optInEmailLightBox'
				    });
					$('#optInEmailOverlay').trigger('click');
				}else{
					$('#emailSuccessMsgHolder').hide();
					$('#emailErrorMsgHolder').show();
					$('#emailErrorMsgHolderInner').html("");
					var errorMessage = data['errors'][0].split(":");
					$('#emailErrorMsgHolderInner').html(errorMessage[2]);
					$spanInContext = $('#emailErrorMsgHolderInner');
					$arrowInContext = $('.footerErrorMsgArrow');
				}
				
				var arrowPos = 0;
				var useWidth = $spanInContext.width();
				if(useWidth/100 > 1){useWidth = useWidth/2 + 16;}
				arrowPos = useWidth + 20;
				$arrowInContext.css('left',-arrowPos)
			}
			
			function optinEmailError(jqXHR, textStatus, errorThrown){
				
			}
        });
        
    </script>    
	
		
		
	
		
		
			
		
	
