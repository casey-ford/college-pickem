	<script> 
        jQuery(document).ready(function() { 
        	$('input.eeemail16').each(function(){
    			if(this.value == ''){
    				this.value='Enter your email address'; 
    			}
    		});
            $('input.eeemail16').click(function(){
    			var inputvalue= this.value;
    			if(inputvalue == 'Enter your email address'){
    				this.value='';
    			}
    		});

        	
        	$('.btnSaveCartImgZip',$('#saveCartOverlayZip')).click(function(e){
        		e.preventDefault();
        		$('.btnSaveCartZip',$('.saveCartSubmitZip')).trigger('click');
        	});
        	$('.btnSaveCartOkImgZip',$('#saveCartOverlayZip')).click(function(e){
        		e.preventDefault();
        		/* if($('#flowType').val() == 'changeLocation'){
        		 */	$('#changeZipEligibility').show();
					$('#saveCartOverlayZip').hide();
					$('#warningSaveCartZip').hide();
				/* }else{
        			$('.close',$('#zipcodeOverlayLightBox')).trigger("click");
        		} */
        	});
        	
        	
        	jQuery.ajaxSetup({accepts:'application/json'}) ;
        	
			jQuery('.btnSaveCartZip',$('.saveCartSubmitZip')).click(function(event) {
			 	var data = {};
		        jQuery('input',$('.saveCartSubmitZip')).each(function(){
		        	data[$(this).attr('name')] = $(this).val();
		        });
		        jQuery('select',$('.saveCartSubmitZip')).each(function(){
		        	data[$(this).attr('name')] = $(this).val();
		        });
				//Pass the name/value pair of submit button as well.
		        data[jQuery('.btnSaveCartZip').attr('name')] = jQuery('.btnSaveCartZip').attr('value');
		        var url = '/smallbusiness/cart/saveCartResult.jsp';
					event.preventDefault();
					jQuery.ajax({
						url : url,
						type : 'POST',
						data : data,
						success : validationSuccess,
						error : validationError,
						dataType : 'json',
						beforeSend : function(xhr) {
							xhr.setRequestHeader('Accept','application/json');
							$('.loadingZip').show();
							$('.saveCartContainerZip').hide();
						}
					});
					
					var wtPN = WebMetrics.getMetaTagValue('DCSext.wtPN');
					var email = $('#emailAddCartZip').val();
					if(email != undefined && email != null){
						WebMetrics.DCSext.wtB2BSaveCartEmailAddress = $('#emailAddCartZip').val();
						WebMetrics.dispatchReport( wtPN + "_SaveCartAndContinueLater");
					}
					return false;
			  });
			function validationSuccess(data){
				$('.loadingZip').hide();
				$('.saveCartContainerZip').show();
				var responseData = data['errors'];
				var isCartSaved = data['alreadyCartSavedZip'];
				if(responseData != undefined){
					if(isCartSaved == true)
					{
						$('#infoDisplayZip').html("");
					    var infoMsg = responseData[0];
					    var infoArray= infoMsg.split(":");
						if(infoArray.length > 1){
							var infoMsgShow = '<div  class="infoMsgDiv" >'+infoArray[1]+'</div>';
						}else{
							var infoMsgShow = '<div  class="infoMsgDiv" >'+infoArray[0]+'</div>';
						}
						$('#infoDisplayZip').html(infoMsgShow);
						$('#serverReponseZip').html("");
						$('#alreadySavedZip').show();
						$('#defaultSubmitZip').hide();
						$('#isUpdateFlagZip').val('true');
						isCartSaved = false;				
						
					}
					else{
						$('#serverReponseZip').html("");
						var errorMsg =responseData[0]
						var errorArray= errorMsg.split(":");
						if(errorArray.length > 1){
							var errorMsgShow = '<span class="inlineErrorMsg" style="color:red;">'+errorArray[1]+'</span>';
						}else{
							var errorMsgShow = '<span class="inlineErrorMsg" style="color:red;">'+errorArray[0]+'</span>';
						}
						$('#serverReponseZip').html(errorMsgShow);
						$('#infoDisplayZip').html("");
						$('#alreadySavedZip').hide();
						$('#defaultSubmitZip').show();
						/* $('#btnSaveCartImg').hide(); */
						$('#isUpdateFlagZip').val('false');
					}
				}
				else{
					$('.loadingZip').hide();
					$('.saveCartContainerZip').show();
					$('#infoDisplayZip').html("");
					$('#initSavedCartMsgZip').hide();
					$('#savedCartMsgZip').show();
				}
			}
			function validationError(jqXHR, textStatus, errorThrown) {
			}
        	
        });
    </script>


	
