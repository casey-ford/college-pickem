									//WebMetrics.DCSext.wtB2BBIDProductFilterMethod = filterMethod;
									//WebMetrics.DCSext.wtB2BEstimatedZipCode = estimatedZipCode;
									
								
							
						
					
			
			
			
			$('#overlayClose').click(function(e){
				
				if(window.WebMetrics){
					WebMetrics.Overlay.DCSext.wtSuccessFlag	= "-1";
					WebMetrics.overlayEventDispatch("SMBID_AddressValidation_Address_Enter");
				}				
			});
			$('#zipcodeOverlayBackDrop').click(function(e){
				
				if(window.WebMetrics){
					WebMetrics.Overlay.DCSext.wtSuccessFlag	= "-1";
					WebMetrics.overlayEventDispatch("SMBID_AddressValidation_Address_Enter");
				}
			});
			
			
			
			
			$('input',$('#serviceZip')).bind('keypress',function (event){
				if (event.keyCode === 13){
					if( $('#optionalzipBtn').is(':visible')) {
					$('#optionalzipBtn').trigger('click');
					}
				}
			});
			
			$('input',$('#showphoneNumberZip')).bind('keypress',function (event){
				if (event.keyCode === 13){
					if( $('#btnChangeAddressZip').is(':visible')) {
					$('#btnChangeAddressZip').trigger('click');
					}
				}
			});
			
			$('input.optional').each(function(){
				if(this.value == ''){
					this.value='Optional'; 
				}
			});
			
			$('input.optional').click(function(){
				var inputvalue= this.value;
				if(inputvalue == 'Optional'){
					this.value='';
				}
			});
			
			$('#loadingZipPocessing').hide();
			
			$('#optionalzipBtn').click(function(e){
				//alert("11");
				e.preventDefault();
				$('#validateZipBtn').trigger('click');
			});
			 $('#btnChangeAddressZip').click(function(e){
				
				e.preventDefault();
				$('#validateZipBtn').trigger('click');
			}); 
			
			// gear implementation 
			 function checkNumber() {
				 if($('#telephoneNumber_text_zip').val().indexOf('_') >-1 || $('#telephoneNumber_text_zip').val() == "" || $('#telephoneNumber_text_zip').val().length < 12) {
					 $('.goButton_enable').hide();
						$('.goButton_disable').show();
					}
					else{
						$('#telephoneNumber_text_zip').val().replace( /(\d{3})(\d{3})(\d{4})/, '$1-$2-$3' );
						$('.goButton_enable').show();
						$('.goButton_disable').hide();
				}
			}
			
			$('#telephoneNumber_text_zip').bind('keydown keyup change focus',checkNumber);
			$('#telephoneNumber_text_zip').mask("?999-999-9999");
			
			
