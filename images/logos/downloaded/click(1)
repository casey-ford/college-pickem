<script> 
	
        jQuery(document).ready(function() {
        	
        	$('.continueWarning').click(function(){
        			$('#changeAdreesEligibility').show();
            		$('#changeZipEligibility').show();
            		$('#saveCartOverlay').hide();
    				$('#warningSaveCart').hide();
    				$('#saveCartOverlayZip').hide();
    				$('#warningSaveCartZip').hide();
    				$('#showphoneNumber').hide();	
				
        	});
        	
        	$('.saveCartWarning').click(function(){
        		$('#changeAdreesEligibility').hide();
        		$('#changeZipEligibility').hide();
				$('#saveCartOverlay').show();
				$('#warningSaveCart').hide();
				$('#saveCartOverlayZip').show();
				$('#warningSaveCartZip').hide();
        	});
        	$('.wirelessPresentInSession').click(function(e){
				$('#validateAddressBtn').trigger('click');
				$('#saveCartOverlay').hide();
				$('#warningSaveCart').hide();
				$('#saveCartOverlayZip').hide();
				$('#warningSaveCartZip').hide();
				$('#showphoneNumber').hide();
			});
        	
        });
</script>        

	
	
