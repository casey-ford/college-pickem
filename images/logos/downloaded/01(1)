                if((pname.indexOf("/homepage/index.html")>=0 || pname.indexOf("/homepage/es/index.html")>=0 || pname=='' || pname=='/') && hname.indexOf(".att.com")>=0){ 
                 	showOrHideUpgradeBanner(); 
                } 
             }); 
        </script> 
