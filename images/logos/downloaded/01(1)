<script> 
	/* 
		US9344 Upgrade Browser Message 
		Get/Set Cookie Function for Upgrade Browser Message Checking 
		Function called in globalnav.war/includes/globalNav.jsp page 
	*/ 
	function getCookie(name) { 
		var dc = document.cookie; 
		var prefix = name + "="; 
		var begin = dc.indexOf("; " + prefix); 
		if (begin == -1) { 
			begin = dc.indexOf(prefix); 
			if (begin != 0) return null; 
		} 
		else 
		{ 
			begin += 2; 
			var end = document.cookie.indexOf(";", begin); 
			if (end == -1) { 
			end = dc.length; 
			} 
		} 
		return unescape(dc.substring(begin + prefix.length, end)); 
	}  
	function acknowledgeBrowserSupportSeen() { 
		document.cookie = "GNSESS=" + '{"UG": ["Unauth"]}' + "; path=/"; 
	} 
	/* 
		US9344 Upgrade Browser Message 
		Browser Detect JS Function 
		Function called in globalnav.war/includes/globalNav.jsp page 
	*/ 
	var BrowserDetect = { 
		init: function () { 
			this.browser = this.searchString(this.dataBrowser) || "An unknown browser"; 
			this.version = this.searchVersion(navigator.userAgent) 
				|| this.searchVersion(navigator.appVersion) 
				|| "an unknown version"; 
			this.OS = this.searchString(this.dataOS) || "an unknown OS"; 
		}, 
		searchString: function (data) { 
			for (var i=0;i<data.length;i++)	{ 
				var dataString = data[i].string; 
				var dataProp = data[i].prop; 
				this.versionSearchString = data[i].versionSearch || data[i].identity; 
				if (dataString) { 
					if (dataString.indexOf(data[i].subString) != -1) 
						return data[i].identity; 
				} 
				else if (dataProp) 
					return data[i].identity; 
			} 
		}, 
		searchVersion: function (dataString) { 
			var index = dataString.indexOf(this.versionSearchString); 
			if (index == -1) return; 
			return parseFloat(dataString.substring(index+this.versionSearchString.length+1)); 
		}, 
		dataBrowser: [ 
			{ 
				string: navigator.userAgent, 
				subString: "Chrome", 
				identity: "Chrome" 
			}, 
			{ 	string: navigator.userAgent, 
				subString: "OmniWeb", 
				versionSearch: "OmniWeb/", 
				identity: "OmniWeb" 
			}, 
			{ 
				string: navigator.vendor, 
				subString: "Apple", 
				identity: "Safari", 
				versionSearch: "Version" 
			}, 
			{ 
				prop: window.opera, 
				identity: "Opera", 
				versionSearch: "Version" 
			}, 
			{ 
				string: navigator.vendor, 
				subString: "iCab", 
				identity: "iCab" 
			}, 
			{ 
				string: navigator.vendor, 
				subString: "KDE", 
				identity: "Konqueror" 
			}, 
			{ 
				string: navigator.userAgent, 
				subString: "Firefox", 
				identity: "Firefox" 
			}, 
			{ 
				string: navigator.vendor, 
				subString: "Camino", 
				identity: "Camino" 
			}, 
			{		// for newer Netscapes (6+) 
				string: navigator.userAgent, 
				subString: "Netscape", 
				identity: "Netscape" 
			}, 
			{ 
				string: navigator.userAgent, 
				subString: "MSIE", 
				identity: "Explorer", 
				versionSearch: "MSIE" 
			}, 
			{ 
				string: navigator.userAgent, 
				subString: "Gecko", 
				identity: "Mozilla", 
				versionSearch: "rv" 
			}, 
			{ 		// for older Netscapes (4-) 
				string: navigator.userAgent, 
				subString: "Mozilla", 
				identity: "Netscape", 
				versionSearch: "Mozilla" 
			} 
		], 
		dataOS : [ 
			{ 
				string: navigator.platform, 
				subString: "Win", 
				identity: "Windows" 
			}, 
			{ 
				string: navigator.platform, 
				subString: "Mac", 
				identity: "Mac" 
			}, 
			{ 
				   string: navigator.userAgent, 
				   subString: "iPhone", 
				   identity: "iPhone/iPod" 
			}, 
			{ 
				string: navigator.platform, 
				subString: "Linux", 
				identity: "Linux" 
			} 
		] 
	}; 
</script> 
<script type="text/javascript"> 
	jQuery(document).ready(function() { 
		/*  
			US9344 Initialize Browser Detect Script  
			Used to detect the browser name, version and OS 
		*/ 
		BrowserDetect.init(); 
		/*  
    		US9344 Close Upgrade Browser Message Container after close icon is clicked 
		*/ 
		jQuery('#ge5p_z0-close-box-container').click(function(){ 
			/* Check if upgrade message browser container is visible */ 
		    if(jQuery('.ge5p_z0-message-container').is(':visible')) 
		    { 
		        /* yes - close it */ 
		        jQuery(".ge5p_z0-message-container").slideToggle(); 
		    } 
		    /* Set GNSESS Cookie */ 
		    acknowledgeBrowserSupportSeen(); 
		}); 
		/*  
		    US9344 Use Modernizr to detect IE browser by hash change feature  
		*/ 
		if (!Modernizr.hashchange) { 
			/*  
		        IE Browsers found so check if 
		        version is less than we support 
		        Updated as of: 4/11/2013  
		    */ 
		    if(BrowserDetect.browser == 'Explorer') { 
		        //if(BrowserDetect.version < 8 && !doesGNSESSCookieExist()) 
				if(BrowserDetect.version < 7) 
		        { 
					var myCookieGNSESS = getCookie("GNSESS"); 
					if (myCookieGNSESS == null) { 
						// cookie doesn't exist so show message 
						jQuery(".ge5p_z0-message-container").slideToggle('slow'); 
					} 
		        } 
		    }  
		} else { 
		    /*  
		        Modern Browsers are found so check if 
		        version is less than we support 
		        Updated as of: 4/11/2013  
		    */ 
		    if(BrowserDetect.browser == 'Firefox') { 
		        if(BrowserDetect.version < 17) 
		        { 
					var myCookieGNSESS = getCookie("GNSESS"); 
					if (myCookieGNSESS == null) { 
						// cookie doesn't exist so show message 
		        jQuery(".ge5p_z0-message-container").slideToggle('slow'); 
		    }  
		        } 
		    }  
		    if(BrowserDetect.browser == 'Chrome') { 
		        if(BrowserDetect.version < 23) 
		        { 
					var myCookieGNSESS = getCookie("GNSESS"); 
					if (myCookieGNSESS == null) { 
						// cookie doesn't exist so show message 
		        jQuery(".ge5p_z0-message-container").slideToggle('slow'); 
		    }    
		} 
		    }    
		} 
	}); 
</script> 
