<div class="parbase leadgeneration section"> 
<script type="text/javascript"> 
	jQuery(document).ready(function($) { 
		var default_value = " Enter your email"; 
		$('.clear-input').focus(function(){ 
			var element = $(this); 
			if (element.val() === default_value) { element.val(""); } 
		}).blur(function(){ 
			var element = $(this); 
			if (element.val() === "") { element.val(default_value); } 
		}); 
	}); 
</script> 
