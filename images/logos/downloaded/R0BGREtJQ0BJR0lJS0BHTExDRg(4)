<script type="text/javascript">
    
    window.popScroll = function(e,re,url){
        if(!re || !url) return false; 
        if (!e) var e = window.event;
        if(e.preventDefault) e.preventDefault(); else event.returnValue = false;
        url += (/.+\?.+\=.*$/.test(url)?"&":"?") + "scrollpop=" + encodeURIComponent(re);
        var h = (!!window.attachEvent? "280" : "300"), //legacy IE
            pop = window.open(url,"_blank","height=" + h + ",location=1,resizable=1,scrollbars=1,toolbar=0,width=500");
    }
    
</script>

                
